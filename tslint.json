{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["**/*.json"]
  },
  "rules": {
    "member-access": false,
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "variable-name": false,
    "one-variable-per-declaration": false,
    "no-empty": false,
    "no-console": false,
    "no-shadowed-variable": false
  }
}
